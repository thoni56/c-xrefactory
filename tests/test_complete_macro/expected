CXREF -xrefactory-II -crlfconversion -crconversion -server -o server-buffer
-olcxcomplet -maxcompls=500 -olinelen=120  -olcursor=53 CURDIR/completions.c -xrefrc CURDIR/.c-xrefrc -p CURDIR
end-of-options

<sync>
<all-completions nofocus=0 len=<n>>MACRO1  :#define MACRO1(xx)
MACRO2  :#define MACRO2(xx)</all-completions>
-exit
end-of-options

