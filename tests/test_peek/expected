-olcxgetprojectname source.c
end-of-options

<sync>
<set-info len=<n>>test_peek</set-info>
-olcxpush -olcursor=4 source.c
end-of-options

<sync>
<goto>
 <position-lc line=1 col=4 len=<n>>CURDIR/source.c</position-lc>
</goto>
-olcxnext source.c
end-of-options

<sync>
<goto>
 <position-lc line=5 col=4 len=<n>>CURDIR/source.c</position-lc>
</goto>
-olcxpushonly -olnodialog -olcursor=56 source.c
end-of-options

<sync>
<goto>
 <position-lc line=6 col=4 len=<n>>CURDIR/source.c</position-lc>
</goto>
-olcxnext source.c
end-of-options

<sync>
<goto>
 <position-lc line=8 col=4 len=<n>>CURDIR/source.c</position-lc>
</goto>
-olcxpoponly -olnodialog source.c
end-of-options

<sync>
-olcxnext source.c
end-of-options

<sync>
<goto>
 <position-lc line=7 col=4 len=<n>>CURDIR/source.c</position-lc>
</goto>
-exit
end-of-options

