-olcxgetprojectname CURDIR/source.c
end-of-options

<sync>
<set-info len=<n>>test_navigation_after_file_modified_long_path</set-info>
-olcxpush -preload CURDIR/source.c CURDIR/source.preload1 -olcursor=50 CURDIR/source.c -p CURDIR
end-of-options

<sync>
<goto>
 <position-lc line=4 col=4 len=<n>>CURDIR/header.h</position-lc>
</goto>
-olcxnext -preload CURDIR/header.h CURDIR/header.preload -olcursor=39 CURDIR/header.h -p CURDIR
end-of-options

<sync>
<goto>
 <position-lc line=7 col=4 len=<n>>CURDIR/header.h</position-lc>
</goto>
-olcxnext -preload CURDIR/header.h CURDIR/header.preload -preload CURDIR/source.c CURDIR/source.modified -olcursor=90 CURDIR/header.h -p CURDIR
end-of-options

<sync>
<goto>
 <position-lc line=7 col=4 len=<n>>CURDIR/source.c</position-lc>
</goto>
