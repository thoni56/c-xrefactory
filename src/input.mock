/* -*- c -*-*/
#include "input.h"
#include <cgreen/mocks.h>

LexInput currentInput;

LexInput makeLexInput(char *read, char *begin, char *write, char *macroName, InputType inputType) {
    LexInput input;
    input.read      = read;
    input.begin     = begin;
    input.write     = write;
    input.macroName = macroName;
    input.inputType = inputType;

    return input;
}
